<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://xmlns.jcp.org/xml/ns/persistence/orm" version="2.1">

<named-query name = "Member.findByUsername">
	<query>
	<CDATA[
		SELECT m
		FROM Member m
		where m.username = :username
	]></CDATA>
	</query>
</named-query>
</entity-mappings>